function disableStyles() {
  var linksArray, stylesArray=new Array();

   // getting styles sheets
   linksArray=document.getElementsByTagName("link");
   for(i=0;i<linksArray.length;i++) {
     if (linksArray[i].getAttribute("rel").indexOf("stylesheet")!=-1){
       stylesArray.push(linksArray[i]);
     }
   }

   // disabling style sheets
   for(i=0;i<stylesArray.length;i++) stylesArray[i].disabled=true;

   // disabling local styles
   stylesArray=document.getElementsByTagName('STYLE');
   for(i=0;i<stylesArray.length;i++) stylesArray[i].disabled=true;
}

